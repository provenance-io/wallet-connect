{
  "name": "example-react-vite",
  "private": true,
  "version": "3.6.0",
  "homepage": "/walletconnect-demo",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean:cache": "rm -rf ./node_modules/.vite",
    "start": "npm run clean:cache && npm i ../provenanceio-walletconnect-js-${npm_package_version}.tgz && npm run dev",
    "start_old": "npm run dev",
    "start:host": "npm run dev --host"
  },
  "dependencies": {
    "@microlink/react-json-view": "1.22.2",
    "@provenanceio/wallet-utils": "2.8.0",
    "@provenanceio/walletconnect-js": "file:../..",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-router-dom": "6.6.2",
    "styled-components": "5.3.6",
    "styled-normalize": "8.0.7"
  },
  "devDependencies": {
    "@types/create-hash": "1.2.2",
    "@types/node": "^16.11.62",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/secp256k1": "4.0.3",
    "@types/styled-components": "5.1.26",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.7.1",
    "typescript": "^4.9.3",
    "vite": "4.2.1",
    "vite-plugin-node-polyfills": "0.7.0",
    "vite-tsconfig-paths": "4.0.7"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  }
}
